    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
